{
  "name": "financeedu-ai-platform",
  "version": "2.0.0",
  "description": "AI-Powered Financial Education Platform with Blockchain Technology and Quantum Computing Simulation",
  "main": "index.html",
  "scripts": {
    "dev": "npx live-server --port=3000 --open=/index.html --ignore=node_modules,src --wait=500",
    "dev:css": "npx tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "build": "tailwindcss -i ./src/input.css -o ./dist/output.css --minify",
    "build-css": "tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "start": "npm run build && npx http-server -p 3000 -o",
    "preview": "npx http-server -p 3000 -o",
    "clean": "rimraf dist/output.css",
    "serve": "npx live-server --port=3000 --open=/index.html",
    "analyze": "npm run build -- --verbose"
  },
  "keywords": [
    "ai-powered",
    "financial-education",
    "blockchain",
    "quantum-computing",
    "neural-networks",
    "defi",
    "nft-certificates",
    "futuristic-design",
    "tailwind-css",
    "glassmorphism",
    "responsive-design",
    "javascript",
    "html5",
    "css3",
    "web3",
    "fintech"
  ],
  "author": "FinanceEdu Team",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "http-server": "^14.1.1",
    "live-server": "^1.2.2",
    "postcss": "^8.5.6",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/financeedu-ai-platform.git"
  },
  "homepage": "https://your-username.github.io/financeedu-ai-platform",
  "engines": {
    "node": ">=16.0.0"
  },
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/your-username/financeedu-ai-platform/issues"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
